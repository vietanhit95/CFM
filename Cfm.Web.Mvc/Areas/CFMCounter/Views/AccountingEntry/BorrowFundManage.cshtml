</div>

<form id="ConfigForm">
    @Html.AntiForgeryToken()
</form>

<script>
    filter.initDateRange(filterConfig.dateRangeOne, null, null, 'right');
    filter.initDateRange(filterConfig.dateRangeTwo, null, null, 'right');
    filter.initDateRange(filterConfig.dateRangeThree, null, null, 'right');
    filter.initDateRange(filterConfig.dateRangeFour, null, null, 'right');
    common.Init();
</script>

