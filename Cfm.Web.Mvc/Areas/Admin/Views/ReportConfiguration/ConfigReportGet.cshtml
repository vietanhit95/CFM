
@model List<Cfm.Web.Mvc.Areas.Admin.Models.ReportConfigViewModel>


<table id="tblcontent" class="table table-striped table-bordered datatables dataTable no-footer table-fixed">
    <thead>
        <tr class="tr_head">
            <th class="w40 text-center">#</th>
            <th class="w80 text-center">Chỉ tiêu</th>
            <th class="text-left">Mã Chỉ tiêu TM</th>
            <th class="w80 text-center">Chỉ tiêu USD</th>
            <th class="w80 text-center">Mã chỉ tiêu lũy kế</th>
            <th class="w80 text-center">Chỉ tiêu USD lũy kế</th>
            <td class="w80"><a href="javascript:void(0)" class="btn btn-primary btn-sm"></a></td>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count > 0)
        {
            for (int j = 0; j < 20; j++)
            {
                var item = Model[j];
                <tr>
                    <td>@((j + 1).ToString("N0"))</td>
                    <td>@item.DisplayName</td>
                    <td>@item.ItemId</td>
                    <td>@Html.CheckBoxFor(x => item.AllowUsd)</td>
                    <td>@Html.CheckBoxFor(x => item.AllowAccumulate)</td>
                    <td>@Html.CheckBoxFor(x => item.AccumulateUsd)</td>
                    <td></td>
                </tr>
            }
        }
    </tbody>
</table>
