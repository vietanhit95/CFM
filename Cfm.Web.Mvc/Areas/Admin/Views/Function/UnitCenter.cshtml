

<ol class="breadcrumb">
    <li><a href="@Url.Action("Index","Home", new{Area = "Admin"})">Trang chủ</a></li>
    <li class="active">Chức năng đơn vị Đầu mối</li>
</ol>

<div class="row">
    @for (int i = 0; i < 12; i++)
    {
        var stt = i + 1;
        <div class="col-xs-3">
            <a class="info-tiles tiles-cfm has-footer" href="javascript:void(0)">
                <div class="tiles-body">
                    <div class="text-center">
                        <i class="fa fa-user fa-2x"></i>
                    </div>
                </div>
                <div class="tiles-footer">
                    <div class="pull-left percent-change">
                        @(stt.ToString() + ". Quản lý người dùng")
                    </div>
                </div>
            </a>
        </div>
    }
</div>
